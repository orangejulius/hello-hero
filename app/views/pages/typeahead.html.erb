<input type="text" id="search"/>
<link rel="stylesheet" href="/assets/typeahead.css" />
<script type="text/javascript">
	$(function(){
		$('#search').typeahead({
			name: 'twitter_verified_users',
			prefetch: 'http://localhost:3000/twitter.json',
      template: '{{name}} @{{screen_name}}',
      engine: Hogan,
			limit: 10
		});
	});
</script>
